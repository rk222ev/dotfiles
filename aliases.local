alias e="emacs -nw"
alias hidden="ls -d .??*"
alias l="ls -l"
alias t="tmux_new || tmux_attach"
alias tl="tmux list-sessions"

## FUNCTIONS ##
tmux_new() {
  tmux new -s $(basename "$PWD")
}

tmux_attach() {
  tmux attach -t $(basename "$PWD")
}

dock() {
  docker-machine start default ; docker-machine env default && eval "$(docker-machine env default)"
}

