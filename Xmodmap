! [key] [shift+key] [mode_switch+key] [mode_switch+shift+key] [alt-graph_key] [alt-graph+shift+key]
clear lock
clear control
keycode 66 = Control_L
add control = Control_L Control_R

! Swap cmd & alt
remove mod1 = Alt_L
remove mod4 = Super_L
keycode 64 = Super_L
keycode 133 = Alt_L
add mod1 = Alt_L
add mod4 = Super_L


